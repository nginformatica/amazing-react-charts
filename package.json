{
  "name": "amazing-react-charts",
  "version": "0.7.10",
  "description": "An amazing react charts package based in echarts",
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://amazing-react-charts.now.sh/",
  "repository": "git@github.com:nginformatica//amazing-react-charts.git",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "sb dev -p 6006",
    "type-check": "tsc --noEmit",
    "build": "yarn pre:build && yarn build:ts && yarn build:babel && yarn",
    "build:ts": "yarn tsc --emitDeclarationOnly",
    "build:babel": "babel src --out-dir dist --extensions '.ts,.tsx'",
    "pre-build": "rm -rf ./dist && mkdir ./dist && yarn postinstall:off && cp package.json ./dist && yarn postinstall:on",
    "format": "prettier -w 'src/**/*.{ts,tsx}'",
    "lint": "eslint -c .eslintrc.yaml 'src/**/*.ts?(x)'",
    "lint:fix": "eslint -c .eslintrc.yaml 'src/**/*.{ts,tsx}'",
    "lint:quiet": "eslint --quiet .eslintrc.yaml 'src/**/*.{ts,tsx}'",
    "pre-publish": "yarn lint --fix && yarn type-check",
    "postinstall:off": "pinst --disable",
    "postinstall:on": "pinst --enable",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "date-fns": "2.30.0",
    "echarts": "4.9.0",
    "echarts-for-react": "3.0.2",
    "ramda": "0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.2",
    "@babel/plugin-transform-runtime": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-styling-webpack": "0.0.5",
    "@storybook/blocks": "7.4.6",
    "@storybook/react": "7.4.6",
    "@storybook/react-webpack5": "7.4.6",
    "@storybook/testing-library": "0.2.2",
    "@types/echarts": "4.9.19",
    "@types/ramda": "0.29.6",
    "@types/react": "18.2.28",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-imports": "2.0.0",
    "docz": "2.3.1",
    "eslint": "8.51.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "0.6.15",
    "pinst": "3.0.0",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.4.6",
    "styled-components": "6.1.0",
    "typescript": "5.2.2",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "react": "18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
