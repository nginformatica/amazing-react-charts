{
  "name": "amazing-react-charts",
  "version": "0.2.4",
  "description": "An amazing react charts package based in echarts",
  "author": "Leonardo Filipe Rigon",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "typings": "build/index.d.ts",
  "keywords": [
    "nginformatica",
    "charts",
    "echarts",
    "react",
    "canvas"
  ],
  "scripts": {
    "start": "tsc && docz dev",
    "build:ts": "tsc",
    "build": "rm -rf build && yarn build:ts",
    "serve": "docz build && docz serve",
    "docz:build": "docz build",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prepack": "yarn build"
  },
  "dependencies": {
    "echarts": "4.6.0",
    "echarts-for-react": "2.0.15-beta.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@types/echarts": "4.4.1",
    "@types/ramda": "0.26.36",
    "@types/react": "16.9.14",
    "brazilian-values": "0.7.0",
    "date-fns": "2.4.1",
    "docz": "2.1.1",
    "eslint": "6.7.2",
    "eslint-config-keepfy": "7.0.0",
    "eslint-plugin-import": "2.18.2",
    "ramda": "0.26.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
