{
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "json.schemas": [
        {
          "fileMatch": ["/package.json"],
          "url": "https://json.schemastore.org/package",
          "schema": true
        }
      ]
  }
